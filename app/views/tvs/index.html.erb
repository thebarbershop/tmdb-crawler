<h1>TV시리즈 정보 조회</h1>

<table>
  <thead>
    <tr>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @tvs.each do |tv| %>
      <tr>
        <td><%= "##{tv.id} " + tv.title %> </td>
        <td><%= link_to '크레딧 조회', tv %></td>
      </tr>
    <% end %>
  </tbody>
</table>

